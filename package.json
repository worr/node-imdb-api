{
  "author": "William Orr <will@worrbase.com> (http://worrbase.com/)",
  "name": "imdb-api",
  "description": "Queries unofficial imdb APIs to get movie and television information from imdb",
  "version": "4.1.1",
  "main": "./lib/imdb.js",
  "types": "./lib/imdb.d.ts",
  "homepage": "https://github.com/worr/node-imdb-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/worr/node-imdb-api"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "prepare": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "test": "nyc --reporter lcov mocha",
    "check": "npm run lint && npm run test",
    "ci": "npm run check && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docs": "typedoc --out ./doc --ignoreCompilerErrors src/imdb.ts",
    "clean": "rm -rf node_modules lib"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "js-beautify": "^1.11.0",
    "mocha": "^7.2.0",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=6.0"
  },
  "keywords": [
    "imdb",
    "movies",
    "search",
    "tv",
    "television"
  ],
  "dependencies": {
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      "ts"
    ]
  }
}
